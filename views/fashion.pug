extends layout

block content
  body
    include sidemenu.pug
    .container-projectcard
      .group-1
        each item, index in projectCardFashionData
          if index >= 0 && index <= 1
            include projectcard.pug
      .group-0
        each item, index in projectCardFashionData
          if index === 2
            include projectcard.pug
      .group-2
        each item, index in projectCardFashionData
          if index >= 3 && index <= 4
            include projectcard.pug
      .group-2
        each item, index in projectCardFashionData
          if index >= 5 && index <= 6
            include projectcard.pug
      .group-0
        each item, index in projectCardFashionData
          if index === 7
            include projectcard.pug
      .group-2
        each item, index in projectCardFashionData
          if index >= 8 && index <= 9
            include projectcard.pug
      .group-2
        each item, index in projectCardFashionData
          if index >= 10 
            include projectcard.pug
          

