extends ../layout

block content
  body.detailed-view
    include ../sidemenu.pug
    .container-projectcard
      .group-5
        .leftover
          each item, index in detailData
            if index === 0
              include ../projectcardClean.pug
      .group-4
        .left
          each item, index in detailData
            if index >= 1 && index <= 2
              include ../projectcardClean.pug
      .group-6
        each item, index in detailData
          if index >=3 && index <= 5
            include ../projectcardClean.pug
      .group-4
          each item, index in detailData
            if index === 6
              include ../projectcardClean.pug
          .right
            each item, index in detailData
              if index === 7
                include ../projectcardClean.pug
      .group-5
        .marginTop
          each item, index in detailData
            if index === 8
              include ../projectcardClean.pug
              